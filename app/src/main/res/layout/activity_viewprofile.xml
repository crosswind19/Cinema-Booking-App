<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_viewprofile">

    <TextView
        android:id="@+id/tv_ProfileEmail"
        android:layout_width="261dp"
        android:layout_height="25dp"
        android:text="Email"
        android:textColor="#090000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.426"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_email1"
        app:layout_constraintVertical_bias="0.042" />

    <TextView
        android:id="@+id/tv_ProfilePhone"
        android:layout_width="158dp"
        android:layout_height="27dp"
        android:text="PhoneNumber"
        android:textColor="#090000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.225"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.067" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="33dp"
        android:layout_height="26dp"
        android:layout_marginBottom="464dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_profilePic"
        app:layout_constraintVertical_bias="0.827"
        app:srcCompat="@drawable/ic_name" />

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="97dp"
        android:layout_height="20dp"
        android:layout_marginBottom="468dp"
        android:text="Full Name"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.03"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/iv_profilePic"
        app:layout_constraintVertical_bias="0.8" />

    <ImageView
        android:id="@+id/iv_email"
        android:layout_width="33dp"
        android:layout_height="26dp"
        android:layout_marginBottom="380dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_ProfileName"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_email" />

    <TextView
        android:id="@+id/tv_email1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="380dp"
        android:text="Email"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toEndOf="@+id/iv_email"
        app:layout_constraintTop_toBottomOf="@+id/tv_ProfileName"
        app:layout_constraintVertical_bias="0.92" />

    <ImageView
        android:id="@+id/iv_phone"
        android:layout_width="33dp"
        android:layout_height="26dp"
        android:layout_marginBottom="296dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_phone" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="135dp"
        android:layout_height="23dp"
        android:layout_marginBottom="292dp"
        android:text="Phone Number"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toEndOf="@+id/iv_phone"
        app:layout_constraintTop_toBottomOf="@+id/tv_ProfileEmail"
        app:layout_constraintVertical_bias="0.777" />

    <Button
        android:id="@+id/btn_changeprofile"
        android:layout_width="135dp"
        android:layout_height="52dp"
        android:layout_marginBottom="132dp"
        android:text="Change Profile Info"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.207"
        app:layout_constraintStart_toEndOf="@+id/btn_changepass"
        app:layout_constraintTop_toBottomOf="@+id/tv_ProfilePhone"
        app:layout_constraintVertical_bias="0.971" />

    <Button
        android:id="@+id/btn_changepass"
        android:layout_width="125dp"
        android:layout_height="54dp"
        android:layout_marginBottom="128dp"
        android:text="CHANGE PASSWORD"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.258"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_ProfilePhone"
        app:layout_constraintVertical_bias="0.943" />

    <Button
        android:id="@+id/btn_edit"
        android:layout_width="163dp"
        android:layout_height="44dp"
        android:layout_marginBottom="76dp"
        android:text="LOGOUT"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_changepass"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tv_ProfileName"
        android:layout_width="117dp"
        android:layout_height="29dp"
        android:layout_marginBottom="408dp"
        android:text="FullName"
        android:textColor="#090000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.193"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_name"
        app:layout_constraintVertical_bias="0.511" />

    <ImageView
        android:id="@+id/iv_profilePic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.091"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>